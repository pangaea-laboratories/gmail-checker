{
  "name": "Gmail Checker",
  "action": {},
  "manifest_version": 3,
  "version": "0.1",
  "description": "Pangaea Laboratories email checker",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "css": ["contentStyle.css"],
      "js": ["sweetalert2.all.min.js", "contentScript.js"]
    }
  ]
}